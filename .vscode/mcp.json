{
	"servers": {
		"supabase": {
			"command": "npx",
			"args": [
				"-y",
				"@supabase/mcp-server-supabase@latest",
				"--project-ref",
				"idsiiayyvygcgegmqcov"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "sbp_a8462bcf4cafe48e39cb1d4742abb385910fbede"
			},
			"type": "stdio"
		},
		"codacy/codacy-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@codacy/codacy-mcp@latest"
			],
			"env": {
				"CODACY_ACCOUNT_TOKEN": "${input:codacy_account_token}",
				"CODACY_CLI_VERSION": "${input:codacy_cli_version}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"io.github.ChromeDevTools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@0.12.1"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.12.1"
		}
	},
	"inputs": [
		{
			"id": "codacy_account_token",
			"type": "promptString",
			"description": "Your Codacy Account API token (from app.codacy.com â†’ Account). Required.",
			"password": true
		},
		{
			"id": "codacy_cli_version",
			"type": "promptString",
			"description": "Optional: pin Codacy CLI version used by local analysis (e.g., 2.3.1).",
			"password": false
		}
	]
}